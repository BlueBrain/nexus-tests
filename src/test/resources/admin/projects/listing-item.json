  "description": "{projId}",
  "base": "http:example.com/{projId}/",
  "vocab": "http:example.com/{projId}/vocab/",
  "apiMappings": [
    {
      "prefix": "nxv-{projId}",
      "namespace": "https://bbp-nexus.epfl.ch/vocabs/nexus/core/terms/v0.1.0/"
