{
  "@context": [
    "https://bluebrain.github.io/nexus/contexts/storage.json",
    "https://bluebrain.github.io/nexus/contexts/resource.json"
  ],
  "@id": "{id}",
  "@type": [
    "Storage",
    "S3Storage"
  ],
  "default": false,
  "maxFileSize" : {maxFileSize},
  "bucket": "{bucket}",
  "endpoint" : "http://minio.dev.nexus.ocp.bbp.epfl.ch",
  "readPermission": "{read}",
  "writePermission": "{write}",
  "_algorithm": "SHA-256",
  "_incoming": "{kgBase}/storages/{project}/{id}/incoming",
  "_outgoing": "{kgBase}/storages/{project}/{id}/outgoing",
  "_self": "{kgBase}/storages/{project}/{id}",
  "_constrainedBy": "https://bluebrain.github.io/nexus/schemas/storage.json",
  "_project": "{kgBase}/projects/{project}",
  "_rev": 1,
  "_deprecated": false,
  "_createdBy": "{iamBase}/realms/internal/users/{user}",
  "_updatedBy": "{iamBase}/realms/internal/users/{user}"
}