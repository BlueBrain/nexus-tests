{
  "@context" : "https://bluebrain.github.io/nexus/contexts/storage.json",
  "@id" : "nxv:myexternalstorage",
  "@type" : [
    "Storage",
    "RemoteDiskStorage"
  ],
  "default" : false,
  "endpoint" : "{storageBase}",
  "folder" : "nexustest",
  "maxFileSize" : 10737418240,
  "readPermission" : "resources/read",
  "writePermission" : "files/write",
  "_algorithm" : "SHA-256"
}
